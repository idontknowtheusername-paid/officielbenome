# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Google Translate API Configuration
# Clé API pour l'API REST (compatible avec tous les builds)
VITE_GOOGLE_TRANSLATE_API_KEY=your_google_translate_api_key

# Autres configurations
VITE_APP_NAME=MaxiMarket
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=development

# Configuration VAPID pour les notifications push
VITE_VAPID_PUBLIC_KEY=your_vapid_public_key
SUPABASE_VAPID_PRIVATE_KEY=your_vapid_private_key

# Configuration des services externes (optionnel)
VITE_GOOGLE_ANALYTICS_ID=your_ga_id
VITE_SENTRY_DSN=your_sentry_dsn

# Configuration du cache et des performances
VITE_CACHE_TTL=300000
VITE_MAX_CACHE_SIZE=50

# Configuration de développement
VITE_DEBUG=true
VITE_LOG_LEVEL=debug

# Configuration API Mistral (pour le chatbot)
MISTRAL_API_KEY=your-mistral-api-key-here 